local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "Save and Run Cords", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

local Tab = Window:MakeTab({
	Name = "Tab 1",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Section = Tab:AddSection({
	Name = "TELEPORT 1"
})

Tab:AddButton({
	Name = "Teleport Save 1",
	Callback = function()
TELEPORTA5 = game.Players.LocalPlayer.Character.HumanoidRootPart.Position
  	end    
})

Tab:AddButton({
	Name = "Teleport 1",
	Callback = function()
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(TELEPORTA5)
  	end    
})

local Section = Tab:AddSection({
	Name = "TELEPORT 2"
})

Tab:AddButton({
